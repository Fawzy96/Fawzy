<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mohamedboltia.cargo.Register_Three_Activity"
    android:background="@drawable/bg">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:id="@+id/relativeLayout20">

        <Button
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:background="@drawable/left_arrow"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:id="@+id/back3"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout20">
        <ImageView
            android:layout_width="@dimen/_85sdp"
            android:layout_height="@dimen/_85sdp"
            android:background="@drawable/logo"
            android:id="@+id/imageView20"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView18"
            android:layout_toEndOf="@+id/textView18"
            android:layout_marginTop="@dimen/_8sdp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tax Card"
            android:textSize="@dimen/_15sdp"
            android:textColor="@color/white"
            android:layout_marginTop="@dimen/_20sdp"
            android:id="@+id/textView20"
            android:layout_below="@+id/imageView20"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginStart="@dimen/_20sdp"
            />


        <EditText
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_30sdp"
            android:background="@drawable/bar"
            android:paddingLeft="@dimen/_15sdp"
            android:id="@+id/edTaxCard"
            android:inputType="numberSigned"
            android:maxLength="11"
            android:layout_below="@+id/textView20"
            android:layout_alignLeft="@+id/textView18"
            android:layout_alignStart="@+id/textView18" />



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tax Register"
            android:textSize="@dimen/_15sdp"
            android:textColor="@color/white"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:id="@+id/textView18"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/edTaxCard"/>



        <EditText
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_30sdp"
            android:background="@drawable/bar"
            android:paddingLeft="@dimen/_15sdp"
            android:id="@+id/edTaxRegister"
            android:maxLength="9"
            android:inputType="number"
            android:layout_below="@+id/textView18"
            android:layout_alignLeft="@+id/textView18"
            android:layout_alignStart="@+id/textView18" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/edTaxRegister"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:text="Specialization"
            android:textSize="@dimen/_15sdp"
            android:textColor="@color/white"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_alignLeft="@+id/textView20"
            android:layout_alignStart="@+id/textView20"
            android:id="@+id/textView13" />
        <Spinner
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_30sdp"
            android:background="@drawable/spinner"
            android:layout_below="@+id/textView13"
            android:layout_alignLeft="@+id/textView13"
            android:layout_alignStart="@+id/textView13"
            android:id="@+id/SPinnerSpecia">

        </Spinner>

        <Button
            android:layout_width="@dimen/_130sdp"
            android:layout_height="@dimen/_40sdp"
            android:background="@drawable/orange_rectangle"
            android:textSize="@dimen/_20sdp"
            android:text="Confirm"
            android:textStyle="bold"
            android:id="@+id/gotologin"
            android:textColor="@color/white"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="@dimen/_30sdp" />
    </RelativeLayout>
</RelativeLayout>